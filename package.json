{
  "name": "sentry-migr8",
  "version": "0.0.0",
  "description": "Run migrations for the Sentry JavaScript SDKs.",
  "main": "index.js",
  "type": "module",
  "repository": "git@github.com:getsentry/sentry-migr8.git",
  "author": "Sentry",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:tsc",
    "lint:eslint": "eslint . --format stylish",
    "lint:tsc": "tsc",
    "lint:prettier": "prettier --check \"{src,test}/**/*.js\"",
    "fix": "pnpm fix:eslint && pnpm fix:prettier",
    "fix:eslint": "eslint . --format stylish --fix",
    "fix:prettier": "prettier --write \"{src,test}/**/*.js\"",
    "dev": "node ./src/index.js"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "globby": "^13.2.2",
    "semver": "^7.5.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@sentry-internal/eslint-config-sdk": "^7.64.0",
    "@types/node": "^18.17.1",
    "@types/semver": "^7.5.0",
    "@types/yargs": "^17.0.24",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "volta": {
    "node": "18.17.1"
  }
}
